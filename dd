    PlayerESPTab:AddToggle('Outlines', {Text = 'Outlines', Default = true}):OnChanged(function(xpoo12)
    Config.Esp.NamesOutline = xpoo12
    Config.Esp.DistanceOutline = xpoo12
    CustomText.Outline = xpoo12
    end)
    
